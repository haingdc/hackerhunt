{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": ["last 2 versions", "ie >= 7"]
        }
      }
    ], 
    "react"
  ],
  "plugins": [
    "transform-object-rest-spread",
    "transform-class-properties",
    [
      "babel-plugin-transform-require-ignore",
      { "extensions": [".css"] }
    ]
  ]
}